<?xml version="1.0" encoding="utf-8" ?>
<odoo noupdate="0">
    <record id="ir_cron_weekly_domains" model="ir.cron">
        <field name="name">Send warning of domains close to their expiration date</field>
        <field name="model_id" ref="domain_manager.model_domain_manager"></field>
        <field name="state">code</field>
        <field name="code">domain.manager._cron_send_expiry_notifications()</field>
        <field name="interval_number">7</field>
        <field name="interval_type">days</field>
        <field name="numbercall">-1</field>
        <field name="active" eval="True" />
    </record>
</odoo>