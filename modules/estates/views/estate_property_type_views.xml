<odoo>
    <data>
        <record id="type_tree_view" model="ir.ui.view">
            <field name="name">property.type.view</field>
            <field name="model">estates.property.type</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="name" />
                </tree>
            </field>
        </record>
        <record id="estate_type_action" model="ir.actions.act_window">
            <field name="name">Property Types</field>
            <field name="res_model">estates.property.type</field>
            <field name="view_mode">tree,form</field>
        </record>
    </data>
</odoo>